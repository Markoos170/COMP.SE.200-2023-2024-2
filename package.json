{
  "name": "software-testing-assignment",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "test": "jest",
    "coverage": "nyc npm test",
    "coveralls": "nyc report --reporter=text-lcov > coverage.lcov && coveralls < coverage.lcov"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "openai": "^4.20.1",
    "nyc": "^15.1.0",
    "coveralls": "^3.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": ["lcov"],
    "testMatch": [
      "**/*.test.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "babel-jest": "^29.7.0",
    "coveralls": "^3.1.1",
    "jest": "^29.7.0",
    "nyc": "^15.1.0"
  }
  
}
